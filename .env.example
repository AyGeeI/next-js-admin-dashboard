# Auth.js Configuration
# Generate a secure random secret with: openssl rand -base64 32
# Or use: https://generate-secret.vercel.app/32
AUTH_SECRET=replace-with-32-char-secret

# Database Configuration
# Local development (SQLite): file:./prisma/dev.db
# Production (PostgreSQL): postgresql://user:password@host:port/database?schema=public
# Production (Neon): postgresql://user:password@ep-xxx.region.aws.neon.tech/dbname?sslmode=require
DATABASE_URL="file:./prisma/dev.db"
